{
  "name": "@lotsof/factory",
  "version": "1.0.0-alpha.5",
  "description": "Useful components factory UI that allows you to build and test your own components easily",
  "type": "module",
  "author": "Olivier Bossel <olivier.bossel@gmail.com> (https://coffeekraken.io)",
  "license": "MIT",
  "scripts": {
    "dev": "tsc --watch & vite --host",
    "clean": "rm -rf src/**/*.js src/**/*.d.ts src/**/*.map",
    "build": "tsc & vite build",
    "postinstall": "npm run install.composer",
    "install.composer": "composer install",
    "publish": "npm publish --access public"
  },
  "dependencies": {
    "@lotsof/advanced-select-element": "^1.0.0-alpha.5",
    "@lotsof/components": "^1.0.0-alpha.5",
    "@lotsof/i18n": "^1.0.0-alpha.5",
    "@lotsof/json-schema-form": "^1.0.0-alpha.5",
    "@lotsof/lit-element": "^1.0.0-alpha.5",
    "@lotsof/sugar": "^1.0.0-alpha.5",
    "@lotsof/sugarcss": "^1.0.0-alpha.5",
    "@types/json-schema": "^7.0.15",
    "lit": "^3.1.3",
    "typescript": "^5.4.5"
  },
  "exports": {
    ".": {
      "node": "./dist/node/_exports.js",
      "browser": "./dist/js/_exports.js"
    }
  },
  "types": "src/shared/factory.types.ts",
  "homepage": "https://lotsof.dev",
  "gitHead": "3a760e37ec10e3f90acac5df9e74cdb09750ff5f"
}
