{
  "name": "@lotsof/components",
  "version": "1.0.0-alpha.97",
  "description": "Components system to share components libraries easily",
  "type": "module",
  "author": "Olivier Bossel <olivier.bossel@gmail.com> (https://coffeekraken.io)",
  "license": "MIT",
  "scripts": {
    "dev": "tsc --watch & vite --host",
    "clean": "rm -rf src/**/*.js src/**/*.d.ts src/**/*.map",
    "factory": "npx lotsof factory.start",
    "build": "tsc && vite build",
    "install.composer": "composer install",
    "publish": "npm publish --access public"
  },
  "dependencies": {
    "@lotsof/carpenter": "file:../carpenter",
    "@lotsof/config": "file:../config",
    "@lotsof/sugar": "file:../sugar",
    "@lotsof/sugarcss": "file:../sugarcss",
    "@lotsof/types": "file:../types",
    "glob": "^10.4.1",
    "inquirer": "^12.3.2",
    "react-dom": "^18.3.1",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.3",
    "typescript": "^5.3.3",
    "vite": "^6.2.2"
  },
  "exports": {
    ".": {
      "node": "./dist/node/_exports.js",
      "browser": "./dist/js/_exports.js"
    }
  },
  "types": "src/shared/_exports.ts",
  "homepage": "https://lotsof.dev",
  "gitHead": "152e8da258e0c14d43ce0e775f2d6b5b2e04f87e"
