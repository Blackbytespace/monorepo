    "dev.advancedSelectElement": "cd packages/advancedSelectElement && npm run dev",
    "dev.cli": "(cd packages/cli && npm run dev)",
    "dev.config": "(cd packages/config && npm run dev)",
    "dev.components": "(cd packages/components && npm run dev)",
    "dev.docblock": "(cd packages/docblock && npm run dev)",
    "dev.docmap": "(cd packages/docmap && npm run dev)",
    "dev.litElement": "(cd packages/litElement && npm run dev)",
